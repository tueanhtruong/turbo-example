{
  "extends": ["//"],
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build", "panda"]
    },
    "panda": {
      "cache": true,
      "outputs": ["./styled-system/**"],
      "inputs": ["./panda.config.ts"]
    },
    "dev": {
      "dependsOn": ["panda"],
      "cache": false,
      "persistent": true
    }
  }
}
